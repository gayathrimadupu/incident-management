{
  "name": "books",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": {
    "type": "git",
    "url": "<Add your repository here>"
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^7.9.0",
    "@sap/cds-hana": "^2",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "@sap/xssec": "^3",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7.9.1",
    "@sap/ux-specification": "^1.120.9",
    "rimraf": "^3.0.2"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-books": "cds watch --open books/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy books --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "requires": {
      "db": {
        "[devlopment]": {
          "kind": "sqlite"
        },
        "[production]": {
          "kind": "hana"
        }
      },
      "[production]": {
        "auth": "xsuaa"
      },
      "[development]": {
        "auth": {
          "kind": "mocked",
          "users": {
            "admin@sap.com": {
              "password": "Initial",
              "ID": "admin@sap.com",
              "roles": [
                "Admin"
              ]
            },
            "user@sap.com": {
              "password": "Initial",
              "ID": "user@sap.com",
              "roles": [
                "User"
              ]
            }
          }
        }
      },
      "auth": "xsuaa"
    }
  },
  "main": "index.js",
  "author": "gayathri"
}
